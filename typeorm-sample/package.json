{
  "name": "typeorm-sample",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "pm2 start process.json",
    "production": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "build": "npm run clean-app && tsc --outDir ./lib",
    "watch": "npm run clean-app && tsc --watch --outDir ./lib",
    "test": "NODE_ENV=test mocha --require espower-typescript/guess \"./src/test/**/*.ts\"",
    "doc": "npm run clean-doc && typedoc ./src --exclude /**/test/**/*.ts --out ./out --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./lib",
    "clean-doc": "rm -rf ./out",
    "tslint": "tslint \"./src/**/*.ts\""
  },
  "engines": {
    "node": ">=8.11.4"
  },
  "dependencies": {
    "@types/config": "^0.0.34",
    "@types/log4js": "^0.0.32",
    "@types/node": "^10.1.2",
    "@types/source-map-support": "^0.4.0",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "debug": "~3.1.0",
    "express": "^4.16.3",
    "js-yaml": "^3.11.0",
    "log4js": "^1.1.1",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "source-map-support": "^0.5.4",
    "typedi": "^0.7.3",
    "typeorm": "0.2.6",
    "typeorm-typedi-extensions": "0.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/power-assert": "^1.5.0",
    "@types/swagger-jsdoc": "0.0.2",
    "espower-typescript": "^8.1.3",
    "mocha": "^5.2.0",
    "node-pre-gyp": "^0.10.0",
    "pm2": "^2.10.4",
    "power-assert": "^1.5.0",
    "swagger-jsdoc": "^1.9.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  }
}
